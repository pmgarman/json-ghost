<div class="indentation-container">
    <span class="attribute">"menu"</span><span class="control inline">:</span>
    <span class="control inline">[</span>
    <div class="row indentation-content">
        <div class="indentation indentation-toggle">
            <div class="indentation-line"></div>
        </div>
        <div class="col-xs-12 col-sm-11 col-md-11 col-lg-11 col-xs-offset-0 col-sm-offset-1 col-md-offset-1 col-lg-offset-1">

            {{#foreach navigation}}
            <div>
                <span class="value nav-{{slug}}{{#if current}} nav-current{{/if}}" role="presentation"><a href="{{url absolute="true"}}">{{label}}</a></span><span class="control inline">,</span>
            </div>
            {{/foreach}}

        </div>
    </div>
    <span class="indentation-ellipsis indentation-toggle">...</span>
    <span class="control inline">]</span><span class="control inline">,</span>
</div>